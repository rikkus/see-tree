{
  "manifest_version": 2,
  "name": "See Tree",
  "description": "Visualize binary tree",
  "version": "1.0.1",
  "icons": {
    "32": "icons/icon.svg",
    "48": "icons/icon.svg",
    "64": "icons/icon.svg",
    "128": "icons/icon.svg"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": [
      "content.js"
    ]
  }],
  "permissions": [
    "activeTab",
    "contextMenus"
  ]
}